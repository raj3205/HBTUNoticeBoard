<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hbtunoticeboard">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/hbtupic"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hbtupic"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StaffActivity"></activity>
        <activity
            android:name=".ui.login.LoginActivity2"
            android:label="@string/title_activity_login2" />
        <activity android:name=".NewsNotification" />
        <activity android:name=".EventActivity" />
        <activity android:name=".TimeTable" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".WelcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>